{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "summary": {
      "type": "text"
    },
    "beganAt": {
      "required": true,
      "type": "datetime"
    },
    "endedAt": {
      "required": false,
      "type": "datetime"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "events"
    },
    "importance": {
      "type": "integer",
      "required": true,
      "max": 10,
      "min": 0
    },
    "media": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "timelines": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::timeline.timeline",
      "mappedBy": "events"
    }
  }
}
